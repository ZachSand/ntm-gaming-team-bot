{
  "name": "ntm-gaming-team-bot",
  "displayName": "NTM Gaming Team Bot",
  "description": "NTM Gaming Team bot",
  "version": "0.0.1",
  "exports": "./src/core/bot.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "type": "module",
  "author": "ZachSand",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "start": "node --loader ts-node/esm ./src/core/bot.ts",
    "production-start": "pm2 start .pm2-process.json",
    "production-stop": "pm2 stop .pm2-process.json",
    "inspect": "nodemon --inspect src/core/bot.ts",
    "test": "jest --passWithNoTests",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "discord.js": "^13.3.1",
    "lowdb": "^3.0.0",
    "pm2": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/lowdb": "^1.0.11",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.4.5",
    "json": "^11.0.0",
    "markserv": "^1.17.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "typescript-transform-paths": "^3.3.1"
  }
}
